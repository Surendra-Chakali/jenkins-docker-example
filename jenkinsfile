pipeline {
    agent any
    tools{
        //maven 'maven1'
        //dockerTool 'docker1'
    }

    stages {
        stage('Code-Compile') {
            steps {
               sh "mvn clean compile"
            }
        }
        
        stage('Code-Build') {
            steps {
               sh "mvn clean install"
            }
        }
        
    }
}
