pipeline {
    agent any
   
    stages {
        stage('Code-Compile') {
            steps {
               sh "mvn clean compile"
            }
        }
        
        stage('Code-Build') {
            steps {
               sh "mvn clean install"
            }
        }

        stage('Deploy to Docker'){
            steps{
                sh  ' docker build -t sampleproject:${BUILD_NUMBER} . '
            }
        }
        
    }
}
